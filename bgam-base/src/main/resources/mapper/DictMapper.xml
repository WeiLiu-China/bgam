<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xdja.base.mapper.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xdja.base.entity.Dict">
        <result column="C_ID" property="id" />
        <result column="C_NAME" property="name" />
        <result column="C_CODE" property="code" />
        <result column="C_TYPE_CODE" property="typeCode" />
        <result column="C_ORDER_NUM" property="orderNum" />
        <result column="C_ICON_URL" property="iconUrl" />
        <result column="N_STATE" property="state" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        C_ID, C_NAME, C_CODE, C_TYPE_CODE, C_ORDER_NUM, C_ICON_URL, N_STATE
    </sql>

</mapper>
